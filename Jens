pipeline {
    agent any
    stages {
        stage('Clone Repository') {
            steps {
                git 'https://github.com/Keerthi26099/demo1_rep.git'
            }
        }
        stage('Set Environment and Run Jenkins') {
            steps {
                bat '''
                set PATH=C:\\Program Files\\Java\\jdk-21\\bin;%PATH%
                cd "C:\\Program Files\\Jenkins"
                java -jar jenkins.war
                '''
            }
        }
    }
}
