pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                script {
                    sh 'git clone https://github.com/Keerthi26099/demo1_rep.git'
                }
            }
        }
        stage('Compile Java Code') {
            steps {
                script {
                    sh 'javac Hello.java'
                }
            }
        }
        stage('Run Java Code') {
            steps {
                script {
                    sh 'java Hello'
                }
            }
        }
    }
}
